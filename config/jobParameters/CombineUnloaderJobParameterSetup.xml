<?xml version="1.0" encoding="UTF-8"?>

<!-- 
	Combine Unloader AI parameters.
	For documentation look up the CpSettingUtil.lua.
-->

<Settings prefixText="CP_combineUnloaderJobParameters_">
	<SettingSubTitle title="ai_parameterGroupTitlePosition" prefix="false">
		<Setting classType="CpAIParameterPositionAngle" name="startPosition" positionParameterType="DRIVE_TO"/>
	</SettingSubTitle>
	<SettingSubTitle title="CP_jobParameters_fieldPosition_title" prefix="false">
		<Setting classType="CpAIParameterPosition" name="fieldPosition" positionParameterType="FIELD_OR_SILO"/>
	</SettingSubTitle>
	<SettingSubTitle title="giantsUnloader">
		<!-- Is the giants ai unloader wanted for unloading? -->
		<Setting classType="AIParameterBooleanSetting" name="useGiantsUnload" defaultBool="false" isDisabled ="hasPipe"/>
		<Setting classType="CpAIParameterUnloadingStation" name="unloadingStation" isDisabled ="hasPipe" generateValuesFunction="generateUnloadingStations"/>
	</SettingSubTitle>
	<SettingSubTitle title="fieldUnload">
		<!-- Is the giants ai unloader wanted for unloading? -->
		<Setting classType="AIParameterBooleanSetting" name="useFieldUnload" defaultBool="false" isDisabled ="hasPipe"/>
		<Setting classType="CpAIParameterPositionAngle" name="fieldUnloadPosition" positionParameterType="UNLOAD" isDisabled ="hasPipe"/>
		<Setting classType="AIParameterSettingList" name="unloadingTipSide" isDisabled ="hasPipe" generateValuesFunction="generateTipSides"/>
	</SettingSubTitle>
</Settings>
